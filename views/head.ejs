</head>

<body>
<div id="navbar">
  <div class="menu"><img src="/img/logo_sthack.png" /></div>
  <% if(current === 'index'){ %>
    <a class="menu btn selected" href="/">Control interface</a>
  <% } else { %>
    <a class="menu btn" href="/">Control interface</a>
  <% } %>
  <% if(current === 'scoreboard'){ %>
    <a class="menu btn selected" href="/scoreboard">Scoreboard</a>
  <% } else { %>
    <a class="menu btn" href="/scoreboard">Scoreboard</a>
  <% } %>
  <% if(typeof auth === 'undefined'){ %>
    <% if(registrationOpen) { %>
      <% if(current === 'register'){ %>
        <a class="menu btn selected" href="/register">Register</a>
      <% } else { %>
        <a class="menu btn" href="/register">Register</a>
      <% } %>
    <% } %>
</div>
  <% } else { %>
<div class="menu right"><span id="team"></span> - <span id="score"></span></div>
</div>
<div id="console">
</div>
<% } %>